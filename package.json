{
	"name": "obsidian-code-import",
	"version": "1.0.0",
	"description": "Import code files with @import syntax. Compatible with Foam.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"code",
		"import",
		"foam"
	],
	"author": "yhao3",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-jest": "^29.12.1",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jest": "^30.2.0",
		"jiti": "2.6.1",
		"ts-jest": "^29.4.6",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
